<?xml version="1.0" encoding="utf-8" ?>
<Application xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Plapp.App">
    <Application.Resources>
        <!-- Maintain order here! It matters. -->
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="View/Styles/Colors.xaml" />
                <ResourceDictionary Source="View/Styles/Fonts.xaml" />
                <ResourceDictionary Source="View/Styles/Texts.xaml" />
                <ResourceDictionary Source="View/Styles/Buttons.xaml" />
                <ResourceDictionary Source="View/Styles/Pages.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>